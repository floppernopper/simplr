<div class="dark_card" align="center">
  <div align="left">
    <%= @outline.html_safe %>
  </div>
</div>

<% if @wiki.body.present? and false %>
  <%= render "card", wiki: @wiki %>
<% end %>

<% if god? %>
  <div class="dark_card">
    <p align="center">
      <%= link_to "New wiki", new_wiki_path, class: :white_link %>
    </p>
  </div>
<% end %>
