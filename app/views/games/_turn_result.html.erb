<% case result[:choice] %>
<% when :attack %>
  <div id="damage_main">
    <div class="game_damage_dealt">
      <%= result[:damage] %> dealt.
    </div>

    <%= link_to reset_game_interface_path, remote: true do %>
      <div class="game_okay">
        Ok
      </div>
    <% end %>
  </div>
<% when :skip %>
  <%= link_to reset_game_interface_path, remote: true do %>
    <div class="game_okay">
      Ok
    </div>
  <% end %>
<% end %>
